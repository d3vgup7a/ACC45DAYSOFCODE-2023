#include <stdio.h>

int main() {
  int t, a, b, x, y;

  scanf("%d", &t);

  while (t--) {
    scanf("%d %d %d %d", &a, &b, &x, &y);

    if (a > b) {
      ((a - b) <= y) ? printf("\nYES") : printf("\nNO");
    }
    else {
      ((b - a) <= x) ? printf("\nYES") : printf("\nNO");
    }
  }
  return 0;
}
