#include <stdio.h>

int main() {
  int t, x;

  scanf("%d", &t);

  while (t--) {
    scanf("%d", &x);

    if (x % 5 == 0) {
      if (x % 10 == 0) {
        printf("\n%d", x/10);
      }
      else {
        (printf("\n%d", (x / 10) + 1));
      }
    }
    else {
      printf("\n-1");
    }
  }
}
